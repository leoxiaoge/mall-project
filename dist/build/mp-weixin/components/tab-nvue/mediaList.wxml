<view class="view"><view class="list-cell view" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['bindClick',['$event']]]]]}}" bindtap="__e"><view class="teng-order-list view"><view class="teng-order-body teng-flex"><view class="teng-order-image"><image class="teng-order-image-src" src="{{options.CommentPics}}"></image><block wx:if="{{showStatus}}"><image class="teng-order-show-status" src="{{status}}"></image></block></view><view class="teng-order-text"><view class="teng-order-title teng-flex-between"><view class="teng-order-name">{{options.ProductName}}</view><view class="teng-order-num">{{"x"+options.num}}</view></view><view class="teng-order-body teng-flex"><view class="teng-order-placard-status">{{options.OrderStatus}}</view><view class="teng-order-number">{{"订单编号："+options.OrderID}}</view></view><view class="teng-order-footer teng-flex-between"><view class="teng-order-price"><text class="teng-order-msg">成交价：</text><text class="teng-order-original">{{options.price}}</text></view><view class="teng-order-status">待付款</view></view></view></view><view class="teng-order-button teng-flex-end"><button class="btn">{{options.ActionButtons}}</button></view></view></view></view>