<view class="content"><block wx:for="{{options}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="show-item"><view class="show-item-userface"><image src="{{item.UserFace}}" class="_img"></image></view><view class="show-item-content"><view class="show-item-user"><view class="show-item-user-nick"><view class="show-item-usernick">{{item.UserNick}}</view><view class="show-item-date">{{item.SendGoodsDateTime}}</view></view><view class="show-item-moneys"><text>成交价</text><text>{{"¥"+item.OrderMoneys}}</text></view></view><view class="show-item-product-name"><text>{{item.ProductName}}</text></view><view class="show-item-comment"><text>{{item.OrderComment.Comment}}</text></view><block wx:for="{{item.ProductPicList}}" wx:for-item="i" wx:for-index="index" wx:key="index"><view class="show-item-img"><image src="{{i}}" data-event-opts="{{[['tap',[['preview',['$0','$1'],[[['options','',index],['ProductPicList','',index]],[['options','',index,'ProductPicList']]]]]]]}}" bindtap="__e" class="_img"></image></view></block></view></view></block></view>