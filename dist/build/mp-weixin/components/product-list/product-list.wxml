<view class="content"><page-head vue-id="1" title="{{title}}" bind:__l="__l"></page-head><view class="uni-product-list"><block wx:for="{{product}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-product"><view data-event-opts="{{[['tap',[['productDetailsTo',['$0'],[[['product','',index,'Active.ID']]]]]]]}}" class="i-product-content" bindtap="__e"><view class="i-image-view"><image class="i-product-image" src="{{item.ProductPicList[0]}}" mode="aspectFit"></image></view><view class="uni-product-title product-title"><text class="active-no">{{"["+item.Active.ActiveNo+"期]"}}</text><text>{{item.ProductTitle}}</text></view><block wx:if="{{item.Active.PrevActiveMoney>0}}"><view class="uni-product-price"><text class="uni-product-text">上期成交：</text><text class="uni-product-price-original">{{"￥"+item.Active.PrevActiveMoney}}</text></view></block><view class="uni-product-title uni-product-price"><text class="uni-product-text">{{item.Active.LastBillUserName}}</text></view></view><view class="uni-product-prompt"><view class="uni-product-time"><text class="uni-product-away">距离开拍</text><uni-countdown vue-id="{{'2-'+index}}" seconds="{{item.seconds}}" bind:__l="__l"></uni-countdown></view><view class="uni-product-button"><button data-event-opts="{{[['tap',[['productDetailsTo',['$0'],[[['product','',index,'Active.ID']]]]]]]}}" class="btn" bindtap="__e">{{activeStatus}}</button></view></view></view></block></view></view>