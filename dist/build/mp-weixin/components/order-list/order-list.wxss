
page {
		width: 100%;
		height: 100%;
}
.view {
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		    -ms-flex-direction: column;
		        flex-direction: column;
		-webkit-box-sizing: border-box;
		        box-sizing: border-box;
}
.list-cell {
		background-color: #fff;
}
.teng-order-list {
		margin: 0 30rpx;
		padding: 20rpx 0;
		border-bottom: 2rpx solid #f4f4f4;
}
.teng-order-image {
		position: relative;
}
.teng-order-image-src {
		width: 200rpx;
		height: 200rpx;
}
.teng-order-show-status {
		position: absolute;
		left: 0;
		top: 0;
		width: 100rpx;
		height: 100rpx;
}
.teng-order-text {
		margin-left: 20rpx;
}
.teng-order-title {
		padding: 20rpx 0;
}
.teng-order-name {
		font-size: 36rpx;
		color: #757575;
		display: -webkit-box;
    overflow: hidden;
    line-height: 1.5;
    -o-text-overflow: ellipsis;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}
.teng-order-num {
		font-size: 32rpx;
		color: #757575;
		margin-left: 20rpx;
}
.teng-order-placard-status {
		min-width: 80rpx;
		font-size: 24rpx;
		line-height: 1.4;
		color: #fe7f00;
		border: 2rpx solid #fe7f00;
		border-radius: 100rpx;
		padding: 2rpx 14rpx;
}
.teng-order-number {
		font-size: 24rpx;
		color: #909090;
		margin-left: 20rpx;
}
.teng-order-msg {
		font-size: 28rpx;
}
.teng-order-original {
		font-size: 32rpx;
		color: #fe7f00;
}
.teng-order-status {
		font-size: 28rpx;
		color: #666666
}
.i-order-footer {
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		-webkit-box-pack: end;
		-webkit-justify-content: flex-end;
		    -ms-flex-pack: end;
		        justify-content: flex-end;
		-webkit-box-align: center;
		-webkit-align-items: center;
		    -ms-flex-align: center;
		        align-items: center;
}
.teng-order-button {
		margin: 10rpx 0 0 10rpx;
}
.teng-order-button button {
		font-size: 28rpx;
		line-height: 1.4;
		color: #fe7f00;
		background-color: #fff;
		border: 2rpx solid #fe7f00;
		border-radius: 100rpx;
		margin: 0;
		padding: 4rpx 14rpx;
}

