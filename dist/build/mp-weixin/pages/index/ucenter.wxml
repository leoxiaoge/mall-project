<view class="content"><block wx:if="{{userInfo}}"><view class="UCenter-bg"><view class="i-no-userInfo-head"><text>欢迎来到腾拍商城</text></view><view class="i-login"><button data-event-opts="{{[['tap',[['loginPath',['$event']]]]]}}" class="btn i-login-button" bindtap="__e">马上登录</button></view></view></block><block wx:else><view class="UCenter-bg"><view class="userInfo-head"><open-data type="userAvatarUrl"></open-data></view><view class="text-xl"><view class="userInfo-txt-name"><open-data type="userNickName"></open-data></view></view></view></block><view class="teng-gold teng-flex-between"><view class="teng-left teng-flex"><text class="icon-gold"></text><text class="teng-ticket">我的入场券</text><text class="teng-number">{{userInfo.Integrals}}</text></view><view class="teng-right teng-recharge"><button data-event-opts="{{[['tap',[['recharge',['$event']]]]]}}" class="btn" bindtap="__e">去充值</button></view></view><view class="teng-order"><view class="teng-order-head teng-flex-between"><view class="teng-my-order"><text class="teng-my-order-text">我的订单</text></view><view data-event-opts="{{[['tap',[['allOrderClick',['$event']]]]]}}" class="teng-all-orders" bindtap="__e"><text class="teng-all-orders-text">全部订单</text><uni-icon vue-id="1" size="{{20}}" color="#bbb" type="arrowright" bind:__l="__l"></uni-icon></view></view><view class="teng-order-list"><uni-grid class="vue-ref" vue-id="2" options="{{lists}}" is-order="{{true}}" show-border="{{false}}" show-out-border="{{false}}" column-num="{{4}}" data-ref="uniGrid" data-event-opts="{{[['^click',[['orderClick']]]]}}" bind:click="__e" bind:__l="__l"></uni-grid></view></view><view class="teng-listItem"><block wx:for="{{listItem}}" wx:for-item="item" wx:for-index="i" wx:key="i"><uni-list vue-id="{{'3-'+i}}" bind:__l="__l" vue-slots="{{['default']}}"><uni-list-item vue-id="{{('4-'+i)+','+('3-'+i)}}" title="{{item.title}}" thumb="{{item.icon}}" navigatePath="{{item.navigateTo}}" bind:__l="__l"></uni-list-item></uni-list></block></view></view>