<view class="content"><view class="i-padding-wrap"><view class="page-section swiper"><view class="page-section-spacing"><swiper class="swiper-box" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}" indicator-active-color="{{indicatorActiveColor}}"><block wx:for="{{swiper}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.AdPicUrl}}" mode="aspectFill"></image></swiper-item></block></swiper></view></view></view><view class="teng-option"><view class="teng-options"></view><view class="teng-options-grid"><uni-grid vue-id="1" options="{{list}}" show-border="{{false}}" show-out-border="{{false}}" column-num="{{4}}" bind:__l="__l"></uni-grid></view></view><view class="uni-swiper-msg"><view class="uni-swiper-msg-icon"><image src="/static/icon/icon_notice.png" mode="widthFix"></image></view><swiper class="uni-swiper-wrapper" vertical="true" autoplay="true" circular="true" interval="3000" display-multiple-items="2"><block wx:for="{{LastTranActiveList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item><view class="i-item-msg"><text class="teng-notice-content">恭喜<text class="i-notice-original">{{item.ProductName}}</text>以<text class="i-notice-original">{{"¥"+item.OrderMoney}}</text>元拍得</text></view></swiper-item></block></block></swiper><view data-event-opts="{{[['tap',[['noticeList',['$event']]]]]}}" class="notice-more" bindtap="__e">更多</view></view><product-list-being vue-id="2" options="{{productList}}" bind:__l="__l"></product-list-being><product-list vue-id="3" options="{{productListIng}}" bind:__l="__l"></product-list></view>