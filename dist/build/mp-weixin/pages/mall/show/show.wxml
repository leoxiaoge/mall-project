<view class="content"><mescroll-uni bind:down="__e" bind:up="__e" vue-id="1" data-event-opts="{{[['^down',[['downCallback']]],['^up',[['upCallback']]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="teng-show-order-list"><view class="teng-show-userinfo"><view class="teng-show-user"><image class="logo" src="{{item.UserFace}}"></image></view><view class="teng-show-title"><view class="teng-show-name">{{item.UserNick}}</view><view class="teng-show-time">{{item.UpdatedTime}}</view></view></view><view data-event-opts="{{[['tap',[['productDetailsTo',['$0','$1'],[[['orderList','',index,'ProductID']],[['orderList','',index,'ActiveID']]]]]]]}}" class="teng-show-order" bindtap="__e"><view class="teng-show-order-images"><image src="{{item.ProductPicList[0]}}"></image></view><view class="teng-show-order-content"><view class="teng-show-order-content-title">{{item.ProductName}}</view><view class="teng-show-order-content-date">{{"成交时间："+item.CreatedTime}}</view><view class="teng-show-order-content-price"><text>成交价：</text><text class="teng-show-order-content-price-text teng-original">{{item.OrderMoneys}}</text></view></view></view><view class="teng-show-content"><view class="teng-show-content-text">{{item.OrderComment.Comment}}</view></view><view class="teng-show-content-images"><block wx:for="{{item.OrderComment.CommentPicList}}" wx:for-item="i" wx:for-index="idx" wx:key="idx"><view class="teng-show-content-image"><image src="{{i}}" data-event-opts="{{[['tap',[['preview',['$0','$1'],[[['orderList','',index],['OrderComment.CommentPicList','',idx]],[['orderList','',index,'OrderComment.CommentPicList']]]]]]]}}" bindtap="__e"></image></view></block></view></view></block></mescroll-uni></view>