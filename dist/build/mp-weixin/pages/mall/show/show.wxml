<view class="content"><block wx:for="{{orderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="teng-show-order-list"><view class="teng-show-userinfo"><view class="teng-show-user"><image class="logo" src="{{item.UserFace}}"></image></view><view class="teng-show-title"><view class="teng-show-name">{{item.UserNick}}</view><view class="teng-show-time">{{item.Updated}}</view></view></view><view class="teng-show-order"><view class="teng-show-order-images"><image src="{{item.ProductPicList[0]}}"></image></view><view class="teng-show-order-content"><view class="teng-show-order-content-title">{{item.ProductName}}</view><view class="teng-show-order-content-date">{{"成交时间："+item.Created}}</view><view class="teng-show-order-content-price"><text>成交价：</text><text class="teng-show-order-content-price-text teng-original">{{item.OrderMoneys}}</text></view></view></view><view class="teng-show-content"><view class="teng-show-content-text">{{item.OrderComment.Comment}}</view></view><view class="teng-show-content-images"><block wx:for="{{item.OrderComment.CommentPicList}}" wx:for-item="i" wx:for-index="idx" wx:key="idx"><view class="teng-show-content-image"><image src="{{i}}"></image></view></block></view></view></block><uni-load-more vue-id="1" status="{{status}}" content-text="{{contentText}}" bind:__l="__l"></uni-load-more></view>