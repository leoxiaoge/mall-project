<view class="container"><block wx:for="{{BillLists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['i-bill',item.IsWin===0?'':'i-active']}}"><view class="i-bill-image i-flex"><image src="{{item.UserNick}}"></image><view class="i-bill-user">{{item.UserMobile}}</view></view><block wx:if="{{item.IsWin===0}}"><view class="i-bill-IsWin">出局</view></block><block wx:else><view class="i-bill-IsWin">预选</view></block><view class="i-bill-province">{{item.Province+item.City}}</view><view class="i-bill-price">{{item.Price}}</view></view></block></view>