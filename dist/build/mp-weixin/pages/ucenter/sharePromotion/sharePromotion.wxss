
.teng-share {
    padding: 40rpx 60rpx;
		border-radius: 10rpx;
}
.teng-share-content {
		background-color: #fff;
		padding: 40rpx 60rpx;
}
.teng-share-code {
		width: 460rpx;
		height: 460rpx;
		text-align: center;
		margin: 10rpx 20rpx 40rpx 20rpx;
}
.teng-share-list {
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		-webkit-justify-content: space-around;
		    -ms-flex-pack: distribute;
		        justify-content: space-around;
		-webkit-box-align: center;
		-webkit-align-items: center;
		    -ms-flex-align: center;
		        align-items: center;
		width: 100%;
}
.teng-share-text {
		text-align: center;
}
.teng-share-text image {
		width: 68rpx;
		height: 68rpx;
}
.teng-share-list-text {
		font-size: 24rpx;
		color: #7a7a7a;
}
.teng-view-commission {
		text-align: center;
		margin-bottom: 40rpx;
}
.teng-redeem button {
		font-size: 36rpx;
		color: #fff;
		background-color: #fe7f00;
		border-radius: 100rpx;
		margin: 0 60rpx;
}

