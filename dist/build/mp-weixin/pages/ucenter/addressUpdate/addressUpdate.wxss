
page {
		background-color: #fff;
}
.i-address {
		padding: 20rpx 30rpx;
}
.i-address-left {
		min-width: 110rpx;
}
.i-address-right {
		margin-left: 40rpx;
}
.i-line {
		border-bottom: 2rpx solid #f4f4f4;
}
.i-address-bottom {
		border-top: 2rpx solid #f4f4f4;
		border-bottom: 2rpx solid #f4f4f4;
		padding: 10rpx 30rpx;
}
.i-add-button button{
		position: fixed;
		bottom: 0;
		width: 100%;
		height: 98rpx;
		line-height: 98rpx;
		font-size: 32rpx;
		color: #fff;
		background-color: #fe7f00;
		border: none;
		border-radius: 0;
}

