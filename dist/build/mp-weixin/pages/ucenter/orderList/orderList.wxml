<view class="uni-tab-bar"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tapTab',['$event']]]]]}}" bindtap="__e">{{tab.name}}</view></block></scroll-view><swiper class="swiper-box" current="{{tabIndex}}" duration="{{300}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadMore',['$0'],['index1']]]]]}}" bindscrolltolower="__e"><block wx:for="{{orderList}}" wx:for-item="newsitem" wx:for-index="index2" wx:key="index2"><block><media-list vue-id="{{'1-'+index2}}" options="{{newsitem}}" data-event-opts="{{[['^close',[['close',['$0',index2],['index1']]]],['^click',[['goDetail',['$0'],[[['orderList','',index2]]]]]]]}}" bind:close="__e" bind:click="__e" bind:__l="__l"></media-list></block></block><view class="uni-tab-bar-loading">{{''+loadingText+''}}</view></scroll-view></swiper-item></swiper></view>