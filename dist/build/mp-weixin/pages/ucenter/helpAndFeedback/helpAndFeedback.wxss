
.teng-help {
		padding: 10rpx 30rpx 0 30rpx;
}
.teng-help-list-text {
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		-webkit-box-align: start;
		-webkit-align-items: flex-start;
		    -ms-flex-align: start;
		        align-items: flex-start;
		padding: 10rpx 30rpx;
}
.teng-help-list-text image{
		width: 36rpx;
		height: 36rpx;
		margin: 10rpx 0;
}
.teng-help-list-text text {
		width: 100%;
		font-size: 28rpx;
		color: #868686;
		margin-left: 20rpx;
}
.teng-feedback {
		position: fixed;
		bottom: 0;
		width: 100%;
		height: 100rpx;
}
.button {
		height: 100rpx;
		font-size: 30rpx;
		color: #616161;
		line-height: 100rpx;
}
.button:after {
		border: none;
		height: 100rpx;
}

