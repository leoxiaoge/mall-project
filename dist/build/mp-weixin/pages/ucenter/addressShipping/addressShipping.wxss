
page {
		background-color: #fff;
}
.i-address-item {
		padding: 20rpx 0;
}
.i-address-user {
		padding: 0 30rpx;
}
.i-address-user {
		line-height: 1.6;
}
.i-address-mobile {
		margin-left: 80rpx;
}
.i-address-text {
		line-height: 2.4;
		padding: 0 30rpx;
}
.i-address-default-text {
		margin-left: 10rpx;
		color: #727272;
}
.i-address-operating {
		color: #727272;
}
.i-address-toast {
		text-align: center;
		padding-bottom: 15rpx;
}
.i-address-edit text {
		margin: 0 10rpx;
}
.i-address-delete {
		margin-left: 10rpx;
}
.i-address-delete text {
		margin: 0 10rpx;
}
.i-edit-address-icon {
		background: url(/static/icon/icon_edit_address.png) center no-repeat;
		background-size: 100%;
		width: 48rpx;
		height: 48rpx;
}
.i-delete-address-icon {
		background: url(/static/icon/icon_delete_address.png) center no-repeat;
		background-size: 100%;
		width: 48rpx;
		height: 48rpx;
}
.i-height {
		height: 98rpx;
}
.i-address-bottom {
		border-top: 2rpx solid #f4f4f4;
		border-bottom: 2rpx solid #f4f4f4;
		padding: 10rpx 30rpx;
}
.i-add-button button{
		position: fixed;
		bottom: 0;
		width: 100%;
		height: 98rpx;
		line-height: 98rpx;
		font-size: 32rpx;
		color: #fff;
		background-color: #fe7f00;
		border: none;
		border-radius: 0;
}

